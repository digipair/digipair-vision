{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "pins",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Pins name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "domain": {
      "type": "string",
      "description": "Domain",
      "$default": {
        "$source": "argv",
        "index": 1
      }
    }
  },
  "required": ["name", "domain"]
}
